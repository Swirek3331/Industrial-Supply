        </Price>

        <Deconstruct time="0.5"/>

        <ItemContainer capacity="20" maxstacksize="32" canbeselected="true" selectkey="Action" hideitems="true" keepopenwhenequipped="true" movableframe="true" msg="itemmsginteractuse">
